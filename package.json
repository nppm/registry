{
  "name": "@nppm/registry",
  "version": "1.0.8",
  "description": "JavaScript Private Package Manager, Registry &amp; Website",
  "main": "dist/index.js",
  "scripts": {
    "test": "ts-node ./src/test.ts",
    "build": "rm -rf ./dist && tsc -d",
    "cli": "ts-node ./src/bin/index.ts",
    "cmd:login": "npm login --registry=http://127.0.0.1:3000/",
    "cmd:adduser": "npm adduser --registry=http://127.0.0.1:3000/",
    "cmd:whoami": "npm whoami --registry=http://127.0.0.1:3000/",
    "cmd:logout": "npm logout --registry=http://127.0.0.1:3000/",
    "cmd:ping": "npm ping --registry=http://127.0.0.1:3000/",
    "cmd:publish": "npm publish --registry=http://127.0.0.1:3000",
    "cmd:token:list": "npm token list --registry=http://127.0.0.1:3000"
  },
  "keywords": [
    "npm",
    "registry"
  ],
  "files": [
    "dist",
    "tempalte",
    "!dist/test.js",
    "!dist/test.d.ts"
  ],
  "bin": {
    "nppm": "./dist/bin/index.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/async-exit-hook": "^2.0.0",
    "@types/buffer-crc32": "^0.2.2",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@types/koa-compose": "^3.2.5",
    "@types/npmlog": "^4.1.4",
    "@types/randomstring": "^1.1.8",
    "@types/semver": "^7.5.0",
    "@types/ssri": "^7.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@evio/visox": "^1.0.1",
    "@evio/visox-http": "^1.0.7",
    "@evio/visox-ioredis": "^1.0.2",
    "@evio/visox-typeorm": "^1.0.2",
    "@evio/visox-typeorm-ioredis-service": "^1.0.2",
    "@types/koa": "^2.13.8",
    "async-exit-hook": "^2.0.1",
    "axios": "^1.4.0",
    "buffer-crc32": "^0.2.13",
